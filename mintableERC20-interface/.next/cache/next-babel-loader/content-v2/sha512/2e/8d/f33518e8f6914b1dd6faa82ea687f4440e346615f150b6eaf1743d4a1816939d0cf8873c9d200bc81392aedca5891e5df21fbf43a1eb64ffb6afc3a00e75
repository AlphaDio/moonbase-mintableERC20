Bal: 0,\n      unsBal: 0,\n      neptBal: 0,\n      plutBal: 0,\n      mercMint: false,\n      venMint: false,\n      erthMint: false,\n      marsMint: false,\n      jupMint: false,\n      satMint: false,\n      unsMint: false,\n      neptMint: false,\n      plutMint: false,\n   };\n\n   async componentDidMount() {\n      this.onUpdate();\n   }\n\n   componentWillUnmount() {\n      clearInterval(this.intervalID);\n   }\n\n   onUpdate = async () => {\n      // Mercury\n      const [mercBal, mercMint] = await this.getBalance(addresses.mercury);\n      // Venus\n      const [venBal, venMint] = await this.getBalance(addresses.venus);\n      // Earth\n      const [erthBal, erthMint] = await this.getBalance(addresses.earth);\n      // Mars\n      const [marsBal, marsMint] = await this.getBalance(addresses.mars);\n      // Jupiter\n      const [jupBal, jupMint] = await this.getBalance(addresses.jupiter);\n      // Saturn\n      const [satBal, satMint] = await this.getBalance(addresses.saturn);\n      // Uranus\n      const [unsBal, unsMint] = await this.getBalance(addresses.uranus);\n      // Neptune\n      const [neptBal, neptMint] = await this.getBalance(addresses.neptune);\n      // Pluto\n      const [plutBal, plutMint] = await this.getBalance(addresses.pluto);\n\n      this.setState({\n         // Set Balance\n         mercBal: mercBal,\n         venBal: venBal,\n         erthBal: erthBal,\n         marsBal: marsBal,\n         jupBal: jupBal,\n         satBal: satBal,\n         unsBal: unsBal,\n         neptBal: neptBal,\n         plutBal: plutBal,\n\n         // Set Last Updated\n         mercMint: mercMint,\n         venMint: venMint,\n         erthMint: erthMint,\n         marsMint: marsMint,\n         jupMint: jupMint,\n         satMint: satMint,\n         unsMint: unsMint,\n         neptMint: neptMint,\n         plutMint: plutMint,\n      });\n\n      this.intervalID = setTimeout(this.onUpdate.bind(this), 5000);\n   };\n\n   getBalance = async (address) => {\n      // Get price data from proxy contract\n      try {\n         const contractInstance = tokenInstance(address);\n         const dec = await contractInstance.decimals();\n         const mint = await contractInstance.canMint();\n         const balance = contractInstance.balanceOf(ethereum.selectedAddress);\n         return [balance, mint];\n      } catch (error) {\n         // Could not fetch price return error\n         console.log(error);\n         return [0, 'N/A'];\n      }\n   };\n\n   render() {\n      return (\n         <div>\n            <h3>Data Price Feed</h3>\n            <p>\n               Information displayed in the following table corresponds to\n               on-chain price data stored in the Moonbase Alpha TestNet!\n            </p>\n            <table className='ui celled table'>\n               <thead>\n                  <tr>\n                     <th>ERC20 Token</th>\n                     <th>Balance</th>\n                     <th>Mintable?</th>\n                  </tr>\n               </thead>\n               <tbody>\n                  <tr>\n                     <td data-label='ERC20 Token'>Mercury</td>\n                     <td data-label='Balance'>$ {this.state.mercBal}</td>\n                     <td data-label='Mintable?'>{this.state.mercMint}</td>\n                  </tr>\n               </tbody>\n               <tbody>\n                  <tr>\n                     <td data-label='ERC20 Token'>Venus</td>\n                     <td data-label='Balance'>$ {this.state.venBal}</td>\n                     <td data-label='Mintable?'>{this.state.venMint}</td>\n                  </tr>\n               </tbody>\n               <tbody>\n                  <tr>\n                     <td data-label='ERC20 Token'>Earth</td>\n                     <td data-label='Balance'>$ {this.state.erthBal}</td>\n                     <td data-label='Mintable?'>{this.state.erthMint}</td>\n                  </tr>\n               </tbody>\n               <tbody>\n                  <tr>\n                     <td data-label='ERC20 Token'>Mars</td>\n                     <td data-label='Balance'>$ {this.state.marsBal}</td>\n                     <td data-label='Mintable?'>{this.state.marsMint}</td>\n                  </tr>\n               </tbody>\n               <tbody>\n                  <tr>\n                     <td data-label='ERC20 Token'>Jupiter</td>\n                     <td data-label='Balance'>$ {this.state.jupBal}</td>\n                     <td data-label='Mintable?'>{this.state.jupMint}</td>\n                  </tr>\n               </tbody>\n               <tbody>\n                  <tr>\n                     <td data-label='ERC20 Token'>Saturn</td>\n                     <td data-label='Balance'>$ {this.state.satBal}</td>\n                     <td data-label='Mintable?'>{this.state.satMint}</td>\n                  </tr>\n               </tbody>\n               <tbody>\n                  <tr>\n                     <td data-label='ERC20 Token'>Uranus</td>\n                     <td data-label='Balance'>$ {this.state.unsBal}</td>\n                     <td data-label='Mintable?'>{this.state.unsMint}</td>\n                  </tr>\n               </tbody>\n               <tbody>\n                  <tr>\n                     <td data-label='ERC20 Token'>Neptune</td>\n                     <td data-label='Balance'>$ {this.state.neptBal}</td>\n                     <td data-label='Mintable?'>{this.state.neptMint}</td>\n                  </tr>\n               </tbody>\n               <tbody>\n                  <tr>\n                     <td data-label='ERC20 Token'>Pluto</td>\n                     <td data-label='Balance'>$ {this.state.plutBal}</td>\n                     <td data-label='Mintable?'>{this.state.plutMint}</td>\n                  </tr>\n               </tbody>\n            </table>\n         </div>\n      );\n   }\n}\n\nexport default Table;\n"]},"metadata":{},"sourceType":"module"}